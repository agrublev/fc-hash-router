{
    "tabWidth": 4,
    "printWidth": 100,
    "singleQuote": false,
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "semi": true,
    "trailingComma": "none",
    "overrides": [
        {
            "files": "**/*.yml",
            "options": {
                "parser": "yaml",
                "tabWidth": 2,
                "useTabs": false
            }
        }
    ]
}
